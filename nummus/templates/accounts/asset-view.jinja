<div
    class="w-full flex justify-between even:bg-grey-50 hover:bg-grey-100 cursor-pointer"
    {# hx-get="{{ url_for("asset.edit", uri=asset["uri"]) }}" #}
    hx-target="#overlay"
    hx-swap="innerHTML"
    hx-include="this">
    <div class="w-[8rem] min-w-[8rem] p-1 truncate">{{ asset["category"] | enum | title }}</div>
    <div class="w-[15rem] min-w-[15rem] p-1 truncate grow">{{ asset["name"] }}</div>
    <div class="w-[8rem] min-w-[8rem] p-1 truncate text-right">{{ asset["end_qty"] | qty }}</div>
    <div class="w-[8rem] min-w-[8rem] p-1 truncate text-right">{{ asset["end_value"] | money }}</div>
    <div class="w-[8rem] min-w-[8rem] p-1 truncate text-right">{{ asset["end_value_ratio"] | percent }}</div>
    <div class="w-[8rem] min-w-[8rem] p-1 truncate text-right">{{ asset["profit"] | money }}</div>
</div>
