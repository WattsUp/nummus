<div class="flex gap-2 items-start">
    <label class="input-outlined grow">
        <input name="q"
               enterkeyhint="search"
               placeholder=""
               value=""
               hx-target="next error"
               hx-trigger="input delay:200ms"
               hx-include="this"
               disabled
               autocomplete="off">
        <div>
            <div>Search</div>
        </div>
        <div>
            <error></error>
        </div>
        <icon>search</icon>
    </label>
    <button class="btn-tonal mt-1.5" disabled>
        <icon>tune</icon>
    </button>
</div>
<div>
    {% for date, transactions in ctx.transactions.items() %}
        <div>
            <div class="txn-header">
                <div>{{ date }}</div>
            </div>
            {% for txn in transactions[:-1] %}
                {% include "/transactions/table-row.jinja" %}
                <hr class="mx-4">
            {% endfor %}
        </div>
        {% with txn = transactions[-1] %}
            {% include "/transactions/table-row.jinja" %}
        {% endwith %}
    {% endfor %}
</div>
