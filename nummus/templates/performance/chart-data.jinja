{% if include_oob %}
  {% with oob = True %}
    {% include "performance/chart-config.jinja" %}
    {% with accounts = chart.accounts %}
      {% include "performance/accounts.jinja" %}
    {% endwith %}
  {% endwith %}
{% endif %}
<script>
  onLoad(() => {
    performance.update(JSON.parse('{{ chart["data"] | tojson(precision=4) }}'));
  });
</script>
