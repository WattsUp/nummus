{% if include_oob %}
    {% with oob = True %}
        {% include "net-worth/chart-config.jinja" %}
    {% endwith %}
{% endif %}
<div id="net-worth-chart-data">
    {% if chart["defer"] %}
        <script>netWorthChart.defer()</script>
    {% else %}
        <script>netWorthChart.update(JSON.parse('{{ chart["data"] | tojson }}'))</script>
    {% endif %}
</div>
