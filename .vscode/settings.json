{
  "cSpell.words": [
    "abortable",
    "AppenderQuery",
    "arcname",
    "clazz",
    "Curlies",
    "elts",
    "endassets",
    "fogtwo",
    "formnovalidate",
    "froms",
    "gevent",
    "gmtime",
    "hashcode",
    "htmx",
    "jsmin",
    "jsonify",
    "kebabed",
    "novalidate",
    "nummus",
    "ondelete",
    "openapi",
    "poolclass",
    "pubkey",
    "pycryptodome",
    "pyopenssl",
    "pytailwindcss",
    "rapidfuzz",
    "Rebalance",
    "requestline",
    "Reswap",
    "Retarget",
    "robo-advising",
    "simplejson",
    "STRINGISH",
    "superseeds",
    "Svgs",
    "tailwindcss",
    "thefuzz",
    "uselist",
    "vals",
    "viztracer",
    "webassets",
    "werkzeug",
    "Woot"
  ],
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "isort.args": [
    "--profile",
    "black",
    "--force-alphabetical-sort-within-sections",
    "--float-to-top"
  ],
  "[jinja-html]": {
    "editor.defaultFormatter": "Glavin001.unibeautify-vscode"
  },
  "[tailwindcss]": {
    "editor.defaultFormatter": "Glavin001.unibeautify-vscode"
  },
  "files.associations": {
    "*.html": "jinja-html",
    "*.css": "tailwindcss"
},
"python.linting.enabled": false
}
